{
  "name": "donejs-justinbmeyer-jshint",
  "version": "0.1.0",
  "description": "Add JSHint and editor config to DoneJS applications",
  "homepage": "http://donejs.com",
  "author": {
    "name": "Justin",
    "email": "justin@bitovi.com",
    "url": "bitovi.com"
  },
  "license": "MIT",
  "main": "lib/",
  "scripts": {
    "test": "npm run jshint && npm run mocha",
    "jshint": "jshint test/. default/index.js --config",
    "mocha": "mocha test/ --timeout 120000",
    "publish": "git push origin --tags && git push origin",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "keywords": [
    "donejs",
    "generator"
  ],
  "dependencies": {
    "lodash": "^4.10.0",
    "yeoman-generator": "^0.22.5"
  },
  "devDependencies": {
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "yeoman-assert": "^2.1.2",
    "yeoman-test": "^1.1.0"
  }
}
